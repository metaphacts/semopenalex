preferredLabels = rdfs:label,skos:prefLabel,<http://purl.org/dc/terms/title>,<http://xmlns.com/foaf/0.1/name>
showClipboard = true
preferredThumbnails = { \n     ?subject <http://xmlns.com/foaf/0.1/depiction> ?1 \n    BIND ( replace(STR(?1)\\, "%28"\\, "("\\, "i") as ?2)\n    BIND ( replace(STR(?2)\\, "%29"\\, ")"\\, "i") as ?3)\n    BIND ( replace(STR(?3)\\, "%2C"\\, "\\,"\\, "i") as ?4)\n    BIND ( replace(STR(?4)\\, "%26"\\, "&"\\, "i") as ?5)\n    BIND ( replace(STR(?5)\\, "%"\\, ""\\, "i") as ?6)\n    \n    BIND (IRI(?6) as ?value)\n    \n},<http://schema.org/thumbnail>
preferredDescriptions = <http://purl.org/dc/terms/abstract>,<http://www.w3.org/2000/01/rdf-schema#comment>,<http://schema.org/description>
